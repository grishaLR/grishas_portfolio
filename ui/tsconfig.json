{
  "compilerOptions": {
    "types": ["@testing-library/jest-dom"],
    "target": "es5",
    "lib": ["dom", "es6"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@consts": ["src/consts"],
      "@consts/*": ["src/consts/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@enums": ["src/enums"],
      "@enums/*": ["src/enums/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"]
    }
  },
  "include": ["./src/**/*", "src"],
  "exclude": ["node_modules"]
}
